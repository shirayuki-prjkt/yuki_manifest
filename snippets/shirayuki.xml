<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote  name="github"
           fetch="https://github.com/" />

  <remote  name="gitlab"
           fetch="https://gitlab.com/" />

  <remote  name="shirayuki"
           fetch="https://github.com/shirayuki-prjkt"
